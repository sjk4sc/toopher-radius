# if a user is not specified in this file, default to authenticating
# that user completely from AD (including Toopher pairing info)
DEFAULT Auth-Type := TOOPHER_AD
  Fall-Through = Yes

# example for completely locally-defined user (no AD interaction, no toopher)
usr_local Cleartext-Password := "p@ssw0rd", Auth-Type := TOOPHER_PAP

# example for completely locally-defined Toopher user (no AD interaction)
usr_local_toopher Cleartext-Password := "p@ssw0rd", Auth-Type := TOOPHER_PAP
  Toopher-Pairing-Id = '5433f012-1442-429a-a4b7-07a38e543e3c'

# example for user defined in AD, with Toopher-Pairing-Id not in AD
usr_ad_local Auth-Type := TOOPHER_AD
  Toopher-Pairing-Id = '5433f012-1442-429a-a4b7-07a38e543e3c'
		
